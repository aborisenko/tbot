<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

        <changeSet id="create request2 table" author="aborisenko">
          <createTable tableName="Request2">
              <column name="_id" type="int">
                <constraints primaryKey="true" nullable="false"/>
              </column>
              <column name="id" type="text">
                  <constraints nullable="false"/>
              </column>
              <column name="requestStr" type="text">
                  <constraints nullable="true"/>
              </column>
              <column name="fio" type="text">
                  <constraints nullable="true"/>
              </column>
              <column name="contact" type="text">
                  <constraints nullable="true"/>
              </column>
              <column name="wallet" type="text">
                  <constraints nullable="true"/>
              </column>
              <column name="meeting" type="text">
                  <constraints nullable="true"/>
              </column>
              <column name="password" type="text">
                  <constraints nullable="true"/>
              </column>
          </createTable>
        </changeSet>

</databaseChangeLog>